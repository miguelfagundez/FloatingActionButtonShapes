<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ChangeShape" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="FloatingActionButtonStyle" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle1</item>
    </style>
    <style name="FloatingActionButtonStyle2" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle2</item>
    </style>

    <style name="FloatingActionButtonStyle3" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle3</item>
    </style>

    <style name="FloatingActionButtonStyle4" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle4</item>
    </style>

    <style name="FloatingActionButtonStyle5" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle5</item>
    </style>

    <style name="FloatingActionButtonStyle6" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle6</item>
    </style>

    <style name="FloatingActionButtonStyle7" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle7</item>
    </style>

    <style name="FloatingActionButtonStyle8" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle8</item>
    </style>

    <style name="FloatingActionButtonStyle9" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/FabStyle9</item>
    </style>

    <!-- Custom styles for FAB -->
    <style name="FabStyle1" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>

    <style name="FabStyle2" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="FabStyle3" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">15%</item>
    </style>

    <style name="FabStyle4" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>

    <style name="FabStyle5" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="FabStyle6" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="FabStyle7" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">20%</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerFamilyTopRight">cut</item>
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="FabStyle8" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">0%</item>
        <item name="cornerSizeTopRight">75%</item>
        <item name="cornerFamilyTopLeft">cut</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">cut</item>
        <item name="cornerFamilyBottomRight">cut</item>
    </style>

    <style name="FabStyle9" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
    </style>
</resources>